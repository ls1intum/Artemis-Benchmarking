{
  "name": "artemis-benchmarking",
  "version": "0.3.2",
  "private": true,
  "description": "Artemis Benchmarking",
  "license": "MIT",
  "author": {
    "name": "Stephan Krusche",
    "email": "krusche@tum.de",
    "url": "https://ase.cit.tum.de/krusche"
  },
  "scripts": {
    "prebuild": "node prebuild.mjs",
    "build": "npm run webapp:prod --",
    "compile:ts:tests": "npm run prebuild && tsc --project tsconfig.spec.json",
    "cleanup": "rimraf build/",
    "postinstall": "husky && patch-package && cp -R node_modules/jest-preset-angular node_modules/@angular-builders/jest/node_modules",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "serve": "npm run start --",
    "services:up": "docker compose -f src/main/docker/services.yml up --wait",
    "start": "npm run prebuild -- --develop && ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "pretest": "npm run lint",
    "test": "npm run prebuild && ng test --coverage --log-heap-usage -w=4",
    "test:watch": "npm run prebuild && npm run test -- --watch",
    "testw8": "ng test --coverage --log-heap-usage -w=8",
    "update": "ncu -i --format group",
    "webapp:build": "npm run prebuild -- --develop && ng build --configuration development",
    "webapp:dev": "npm run prebuild -- --develop && ng serve",
    "webapp:prod": "npm run prebuild && NG_BUILD_OPTIMIZE_CHUNKS=1 ng build --configuration production"
  },
  "config": {
    "default_environment": "prod"
  },
  "overrides": {
    "braces": "3.0.3",
    "debug": "4.4.1",
    "esbuild": "0.25.9",
    "express": "5.1.0",
    "jsdom": "26.1.0",
    "postcss": "8.5.6",
    "punycode": "2.3.1",
    "rimraf": "6.0.1",
    "tmp": "0.2.5",
    "tough-cookie": "5.1.2",
    "vite": "7.1.2",
    "webpack": "5.101.1",
    "webpack-dev-middleware": "7.4.2",
    "webpack-dev-server": "5.2.2",
    "word-wrap": "1.2.5",
    "ws": "8.18.3",
    "yargs-parser": "22.0.0"
  },
  "dependencies": {
    "@angular/cdk": "20.1.6",
    "@angular/common": "20.1.7",
    "@angular/compiler": "20.1.7",
    "@angular/core": "20.1.7",
    "@angular/forms": "20.1.7",
    "@angular/localize": "20.1.7",
    "@angular/platform-browser": "20.1.7",
    "@angular/platform-browser-dynamic": "20.1.7",
    "@angular/router": "20.1.7",
    "@angular/service-worker": "20.1.7",
    "@fortawesome/angular-fontawesome": "3.0.0",
    "@fortawesome/fontawesome-svg-core": "7.0.0",
    "@fortawesome/free-solid-svg-icons": "7.0.0",
    "@ng-bootstrap/ng-bootstrap": "19.0.1",
    "@stomp/rx-stomp": "2.0.1",
    "@swimlane/ngx-charts": "23.0.0",
    "bootstrap": "5.3.7",
    "d3": "7.9.0",
    "dayjs": "1.11.13",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "20.0.0",
    "@angular/build": "20.1.6",
    "@angular/cli": "20.1.6",
    "@angular/compiler-cli": "20.1.7",
    "@types/d3-scale": "4.0.9",
    "@types/d3-selection": "3.0.11",
    "@types/d3-shape": "3.1.7",
    "@types/jest": "29.5.14",
    "@types/node": "24.2.1",
    "angular-eslint": "20.1.1",
    "buffer": "6.0.3",
    "concurrently": "9.2.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.6.1",
